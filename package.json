{
    "name": "qflow-pro",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@10.26.2",
    "description": "QFlow Pro - ERP SaaS Multi-Tenant",
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "start": "turbo run start",
        "start:prod": "turbo run start:prod",
        "test": "turbo run test",
        "test:e2e": "turbo run test:e2e",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "dev:admin": "pnpm --filter admin dev",
        "dev:portal": "pnpm --filter portal dev",
        "dev:api": "pnpm --filter api start:dev",
        "build:admin": "pnpm --filter admin build",
        "build:portal": "pnpm --filter portal build",
        "migration:generate": "pnpm --filter @qflow/database migration:generate",
        "migration:run": "pnpm --filter @qflow/database migration:run",
        "migration:revert": "pnpm --filter @qflow/database migration:revert",
        "migration:show": "pnpm --filter @qflow/database migration:show",
        "seed": "pnpm --filter @qflow/database seed"
    },
    "keywords": [
        "erp",
        "saas",
        "multi-tenant",
        "pos"
    ],
    "author": "QFlow Pro Team",
    "license": "UNLICENSED",
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "prettier": "^3.7.4",
        "turbo": "^2.7.4",
        "typescript-eslint": "^8.53.0"
    }
}