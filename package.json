{
    "name": "qflow-pro",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@10.26.2",
    "description": "QFlow Pro - ERP SaaS Multi-Tenant",
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "test": "turbo run test",
        "test:e2e": "turbo run test:e2e",
        "lint": "turbo run lint",
        "dev:admin": "pnpm --filter admin dev",
        "dev:portal": "pnpm --filter portal dev",
        "dev:api": "pnpm --filter api start:dev",
        "build:admin": "pnpm --filter admin build",
        "build:portal": "pnpm --filter portal build",
        "migration:generate": "pnpm --filter @qflow/database migration:generate",
        "migration:run": "pnpm --filter @qflow/database migration:run",
        "migration:revert": "pnpm --filter @qflow/database migration:revert",
        "migration:show": "pnpm --filter @qflow/database migration:show"
    },
    "keywords": [
        "erp",
        "saas",
        "multi-tenant",
        "pos"
    ],
    "author": "QFlow Pro Team",
    "license": "UNLICENSED",
    "devDependencies": {
        "turbo": "^2.7.4"
    }
}